        "prettier": "^2.8.4",
        "prettier-plugin-svelte": "^2.9.0",
        "sass": "^1.59.3",
        "svelte": "^3.56.0",
        "svelte-check": "^3.1.4",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
